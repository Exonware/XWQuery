{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "showPresets": {
      "type": "boolean",
      "default": true
    },
    "presets": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "hours": {
            "type": "number"
          },
          "minutes": {
            "type": "number"
          }
        }
      }
    },
    "className": {
      "type": "string"
    }
  },
  "id": "XWUIDurationPicker"
}
