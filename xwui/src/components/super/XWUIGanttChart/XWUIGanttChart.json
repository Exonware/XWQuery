{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "zoomLevel": {
      "type": "string",
      "enum": [
        "day",
        "week",
        "month",
        "year"
      ],
      "default": "week"
    },
    "showSidebar": {
      "type": "boolean",
      "default": true
    },
    "showDependencies": {
      "type": "boolean",
      "default": true
    },
    "showCriticalPath": {
      "type": "boolean",
      "default": false
    },
    "className": {
      "type": "string"
    }
  },
  "id": "XWUIGanttChart"
}
