@stylesheet = {{#each rules}}{{value}}
{{/each}}

@rule = {{selector}} {
{{#each declarations}}{{value}}
{{/each}}}

@selector = {{#each selectors}}{{value}}{{#if !@last}}, {{/if}}{{/each}}

@declaration = {{property}}: {{value}}{{#if important}} !important{{/if}};

@indent = 2
@pretty = true

